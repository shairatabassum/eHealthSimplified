        <activity android:name=".Appointment_doctor_main" />
        <activity android:name=".FindDoctor_main" />
        <activity android:name=".Registration_Doctor" />
        <activity android:name=".AppointmentRequest" />
        <activity android:name=".DoctorReview_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>